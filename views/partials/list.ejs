<div class="list">
    <p>Blog posts:</p>
    <ul>
        <% Object.entries(newListItem).forEach(([key, value])=> { %>
            <li>
                <a href="/blogpost?key=<%= key %>">
                    <%= key %>
                </a>
                <a href="/edit?key=<%= key %>"> <img src="/icons/edit-flip-h.svg" alt="Edit Icon">
                    <!-- Edit icon -->
                </a>
                <a href="/delete?key=<%= key %>"> <img src="/icons/trash.svg" alt="Trash Icon">
                    <!-- Trash icon -->
                </a>
            </li>
            <% }); %>
    </ul>
</div>